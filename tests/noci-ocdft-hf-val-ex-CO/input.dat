#NOCI with OCDFT determinant CO val excitation energy
#sys.path.insert(0, './..')
#sys.path.insert(0, '/home/prakash')
import cdft 

molecule h2o {
C       0.0000  0.0000  0.0000
O       0.0000  0.0000  1.1282  
symmetry c1
}


set {
  basis cc-pvdz
  df_scf_guess false
  scf_type pk
#  e_convergence 6 
  d_convergence 1
  maxiter 100
}

set reference uks
set cdft {
  cdft_exc_type valence
  nroots 2
  cdft_spin_adapt_ci false
  cdft_exc_method chp
  cdft_num_project_out 5
  cdft_project_out HP
}

set dft_functional hfexch
energy('ocdft')


